<h1>List of all directors</h1>

<table border="1">
<% @list_of_directors.each do |data| %>
  <tr>
    <td><%= data.id %></td>
    <td>
      <img src="http://robohash.org/<%= data.image %>">
    </td>
    <td><%= data.name %></td>
    <td><%= data.dob %></td>
    <td>
      <a href="/directors/<%= data.id %>">
      Show details
    </td>
  </tr>
<% end %>
</table>